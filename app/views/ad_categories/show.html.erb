      <dt class="label label-default"><%= model_class.human_attribute_name(:ad_text3) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.ad_text3 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ad_text4) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.ad_text4 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ad_text5) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.ad_text5 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_url1) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.fast_url1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_url2) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.fast_url2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_url3) %></dt>
