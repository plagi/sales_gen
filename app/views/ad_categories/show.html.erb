      <dt class="label label-default"><%= model_class.human_attribute_name(:ad_texts) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.ad_texts %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_urls) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.fast_urls %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:deleted_at) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.deleted_at %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:ad_text1) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.ad_text1 %></pre></dd>
