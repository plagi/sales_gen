<%- model_class = AdCategory -%>
<div class="page-header">
  <h1><%=t '.title', :default => AdCategory %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:url) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.url %></pre></dd>
