      <dd><pre class="prettyprint"><%= @ad_category.fast_url_title1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_url_title2) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.fast_url_title2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_url_title3) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.fast_url_title3 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:fast_url_title4) %></dt>
      <dd><pre class="prettyprint"><%= @ad_category.fast_url_title4 %></pre></dd>
